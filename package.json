{
  "name": "myshop",
  "version": "2.0.0",
  "description": "mern ecommerce",
  "private": true,
  "type": "module",
  "main": "server.js",
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "build": "npm run build --workspace frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "author": "Fatima Ahmadinejad",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.6.1",
    "mongodb": "4.1",
    "mongoose": "^8.7.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.7"
  }
}
